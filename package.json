{
  "name": "orderly-ai-architect",
  "version": "0.1.0",
  "private": true,
  "description": "Orderly AI Architect - AI-powered DEX construction platform",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "build:vercel": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
