    "generate:registry": "node scripts/generate-registry.mjs"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "prettier": "^3.3.3",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
